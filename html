		var wrong = [];
		
		var correct = [];
		
		var correctD = [];
		
		var wrongD = [];
		
		var score = 0;
		
		function resetScore () {
			score = 0;
			
		}
		
		function initialize () {
			wrong = [];
			correct = [];
	
			resetScore();
			playGame ("buw5");
			
		}
		
		function wordsWC () {
			for (var i = 0; i < 20; i++){
				var ir = "x" + i;
				document.getElementById(ir).innerHTML = " ";
			
			}
		
			for (var i = 0; i < correct.length; i++){
				var er = "x" + i;
				document.getElementById(er).innerHTML = correct[i];
			
			}
			
			for (var i = 0; i < 20; i++){
				var or = "h" + i;
				document.getElementById(or).innerHTML = " ";
			
			}
		
			for (var i = 0; i < wrong.length; i++){
				var ar = "h" + i;
				document.getElementById(ar).innerHTML = wrong[i];
			
			}
		
		}
		
		
		function playGame (id) {
			
			//experiment Part 2
			 
			
			if (score >= 2400) {
				window.location.href = "#winning";
			}
			
			
			if (id == "buw" + document.getElementById("secretK").innerHTML){
				alert("correct!");
				
				correct.push(document.getElementById("secretC").innerHTML);
				correctD.push(document.getElementById("secretA").innerHTML);
				
				score = score + 100;
			}
			
			
			
			else if (id == "buw5"){
				var w;
			}
			
			else {
				alert("wrong");
				wrong.push(document.getElementById("secretC").innerHTML);
				wrongD.push(document.getElementById("secretA").innerHTML);
				score = score - 25;
			}
			
			
			
			
			//code to display definition
			var a = Math.floor((Math.random() * 67) + 0);
			document.getElementById("secretA").innerHTML = a;
			document.getElementById("defin").innerHTML = defi[a];
			
			// code to set button values
			for(var j = 1; j < 5; j++){ 				
				var r = Math.floor((Math.random() * 67) + 0);
				if (r == a){
					var r = Math.floor((Math.random() * 67) + 0);
				}	
						
				var bucID= "bu"+j;
				document.getElementById(bucID).innerHTML = words[r];	
			}
			var k = Math.floor((Math.random() * 4) + 1);
			var buID= "bu"+k;
			//k is the number of the correct answer button
			//buID is the ID to the correct answer button
			document.getElementById(buID).innerHTML = words[a];
			document.getElementById("secretC").innerHTML = words[a];
			
			//experiment
			document.getElementById("secretK").innerHTML = k;
			document.getElementById("score").innerHTML = score;
	

		}
	
