		<script>
		$("#buw5").click(function () {
			var sec=0;
			var min=0;
			setInterval(function(){
				sec++;
				if ( sec==62){
					min++;
					
				}
				if (sec > 58){
				//<button onclick='foo'>Go</button>
					window.location.href = "#last"; 
					sec = 0; 
				
				}
				else{
					document.getElementById("timer").innerHTML= sec; 

						
				} 
				
				
				
			},1000);
			
		});
		
	    
		</script>
		
		
		
	
	
		

